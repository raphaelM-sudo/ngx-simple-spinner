{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "simple",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "simple",
      "kebab-case"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "no-input-rename": false,
    "no-output-rename": false
  }
}
